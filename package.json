{
  "name": "linq-to-typescript",
  "version": "4.0.0-alpha2",
  "description": "LINQ ported to TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "maintainers": [
    {
      "name": "Alexandre Rogozine",
      "email": "alexandre.rogozine@live.com"
    }
  ],
  "scripts": {
    "postinstall": "node scripts/init",
    "build": "tsc --target es5 --downlevelIteration --target dist/esm5 && tsc && rollup -c config/rollup.config.js && rollup -c config/rollup.config.js --environment NODE_ENV:production",
    "test": "jest -c ./test/jest.config.js --runInBand",
    "lint": "tslint -p ./",
    "doc": "typedoc ./src --out ./doc --excludePrivate --readme none --ignoreCompilerErrors --name \"LINQ To TypeScript\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arogozine/LinqToTypeScript.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "linq",
    "async",
    "generators",
    "enumerable",
    "es2016"
  ],
  "author": "Alexandre Rogozine",
  "license": "MIT",
  "dependencies": {
    "core-js": "^2.5.5"
  },
  "peerDependencies": {
    "tslib": ">=1.9.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "jest": "^23.6.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.3",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2",
    "webpack-config-utils": "^2.3.1"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "bugs": {
    "url": "https://github.com/arogozine/LinqToTypeScript/issues"
  },
  "homepage": "https://github.com/arogozine/LinqToTypeScript#readme"
}
