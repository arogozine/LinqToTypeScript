{
  "name": "linq-to-typescript",
  "version": "10.0.0-beta1",
  "description": "LINQ ported to TypeScript",
  "type": "module",
  "main": "./commonjs/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./commonjs/index.js",
      "default": "./esm/index.js"
    }
  },
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "maintainers": [
    {
      "name": "Alexandre Rogozine",
      "email": "alexandre.rogozine@live.com"
    }
  ],
  "scripts": {
    "build": "npx rimraf dist && npm run build:main && npm run build:esm && npm run build:declaration",
    "build:main": "tsc --removeComments --module commonjs --outDir dist/commonjs",
    "build:declaration": "tsc --module commonjs --outDir dist/types --declaration --emitDeclarationOnly",
    "build:esm": "tsc --removeComments --outDir dist/esm",
    "postbuild": "node config/copy && node config/fix-imports",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src",
    "doc": "npx rimraf doc && typedoc ./src --exclude \"**/_*/*.ts\" --exclude \"**/bindArrayEnumerable.ts\" --gitRevision master --out ./doc --excludePrivate --readme README.md --name \"LINQ To TypeScript\" --includeVersion && node config/editdocs",
    "release:npm": "npm publish ./dist",
    "release:npm-next": "npm publish ./dist --tag next",
    "release:pack": "npm pack ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arogozine/LinqToTypeScript.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "linq",
    "async",
    "generators",
    "enumerable",
    "es2019"
  ],
  "author": "Alexandre Rogozine",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-plugin-jsdoc": "^37.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=17"
  },
  "bugs": {
    "url": "https://github.com/arogozine/LinqToTypeScript/issues"
  },
  "homepage": "https://github.com/arogozine/LinqToTypeScript#readme"
}
