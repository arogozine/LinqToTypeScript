{
  "name": "linq-to-typescript",
  "version": "4.0.0-alpha1",
  "description": "LINQ ported to TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "maintainers": [
    {
      "name": "Alexandre Rogozine",
      "email": "alexandre.rogozine@live.com"
    }
  ],
  "scripts": {
    "build": "tsc",
    "test": "node ./node_modules/karma/bin/karma start karma.conf.js",
    "quick-test": "./node_modules/.bin/karma start --browsers Chrome --single-run",
    "lint": "tslint -p ./",
    "doc": "typedoc ./src --out ./doc --excludePrivate --readme none --ignoreCompilerErrors --name \"LINQ To TypeScript\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arogozine/LinqToTypeScript.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "linq",
    "async",
    "generators",
    "enumerable",
    "es2016"
  ],
  "author": "Alexandre Rogozine",
  "license": "MIT",
  "dependencies": {
    "core-js": "^2.5.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "jasmine-core": "^3.1.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-typescript": "^3.0.13",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "bugs": {
    "url": "https://github.com/arogozine/LinqToTypeScript/issues"
  },
  "homepage": "https://github.com/arogozine/LinqToTypeScript#readme"
}
