{
  "name": "linq-to-typescript",
  "version": "4.0.0-alpha2",
  "description": "LINQ ported to TypeScript",
  "main": "./lib/index.js",
  "module": "./module/index.esm.js",
  "es2015": "./bundles/index.esm.min.js",
  "browser": "./bundles/index.cjs.min.js",
  "typings": "./types/index.d.ts",
  "maintainers": [
    {
      "name": "Alexandre Rogozine",
      "email": "alexandre.rogozine@live.com"
    }
  ],
  "scripts": {
    "build:module": "tsc --target esnext --module esnext --outDir dist/module",
    "build:main": "tsc --module commonjs --target es2016 --outDir dist/lib",
    "build:es2015": "tsc --outDir dist/esm2015",
    "build:cjs": "tsc --module commonjs --target es5 --downlevelIteration --outDir dist/cjs",
    "build:rollup": "rollup -c config/rollup.config.js --environment NODE_ENV:production",
    "build": "npm run build:module && npm run build:main && npm run build:cjs && npm run build:es2015 && npm run build:rollup",
    "postbuild": "node scripts/copy",
    "test": "jest -c ./test/jest.config.js --forceExit --detectOpenHandles --maxWorkers=10",
    "lint": "tslint -p ./",
    "doc": "typedoc ./src --out ./doc --excludePrivate --readme none --ignoreCompilerErrors --name \"LINQ To TypeScript\"",
    "release:npm": "npm publish dist",
    "release:preflight": "npm pack ./dist --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arogozine/LinqToTypeScript.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "linq",
    "async",
    "generators",
    "enumerable",
    "es2016"
  ],
  "author": "Alexandre Rogozine",
  "license": "MIT",
  "dependencies": {
    "core-js": "^2.5.5"
  },
  "peerDependencies": {
    "tslib": ">=1.9.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/webpack-config-utils": "^2.3.0",
    "jest": "^23.6.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.3",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2",
    "webpack-config-utils": "^2.3.1"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "bugs": {
    "url": "https://github.com/arogozine/LinqToTypeScript/issues"
  },
  "homepage": "https://github.com/arogozine/LinqToTypeScript#readme"
}
